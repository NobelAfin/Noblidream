<div class="container">
 <h1 class= "text-center" >Administrador</h1>
 <%= render 'admin/nav_links' %>

 <table class="table table-dark table-striped">
    <thead>
        <tr>
        <th scope="col">User_id</th>
        <th scope="col">Email</th>
        <th scope="col">Name</th>
        <th scope="col">Username</th>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
        <tr>
         <th> <%= user.user_id %> </th>
         <td> <%= link_to user.title.truncate(15), admin_user_path(user) %> </td>
         <td> <%= user.name %> </td>
        </tr>
        <% end %> 
    </tbody>
 </table>

</div>